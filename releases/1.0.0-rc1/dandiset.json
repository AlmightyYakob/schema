{
  "title": "PublishedDandiset",
  "description": "A body of structured information describing a DANDI dataset.",
  "type": "object",
  "properties": {
    "schemaVersion": {
      "title": "Schemaversion",
      "default": "1.0.0-rc1",
      "readonly": true,
      "type": "string"
    },
    "identifier": {
      "title": "Identifier",
      "readonly": true,
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "string",
          "minLength": 1,
          "maxLength": 65536,
          "format": "uri"
        },
        {
          "allOf": [
            {
              "$ref": "#/definitions/PropertyValue"
            }
          ]
        }
      ]
    },
    "name": {
      "title": "Title",
      "description": "Name of this dataset",
      "maxLength": 150,
      "type": "string"
    },
    "description": {
      "title": "Description",
      "description": "A description of this dataset",
      "maxLength": 3000,
      "type": "string"
    },
    "contributor": {
      "title": "Contributors",
      "description": "The people or organizations who havecontributed to creating this dataset",
      "type": "array",
      "items": {
        "anyOf": [
          {
            "$ref": "#/definitions/Person"
          },
          {
            "$ref": "#/definitions/Organization"
          }
        ]
      }
    },
    "license": {
      "title": "License",
      "description": "A license document that applies to this content, typically indicated by URL.",
      "nskey": "schema",
      "type": "array",
      "items": {
        "enum": [
          "https://creativecommons.org/publicdomain/zero/1.0/",
          "https://creativecommons.org/licenses/by/4.0/",
          "https://creativecommons.org/licenses/by-nc/4.0"
        ]
      }
    },
    "keywords": {
      "title": "Keywords",
      "description": "Keywords or tags used to describe this content. Multiple entries in a keywords list are typically delimited by commas.",
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "about": {
      "title": "About",
      "type": "array",
      "items": {
        "$ref": "#/definitions/About"
      }
    },
    "studyTarget": {
      "title": "Studytarget",
      "type": "array",
      "items": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "string",
            "minLength": 1,
            "maxLength": 65536,
            "format": "uri"
          }
        ]
      }
    },
    "protocol": {
      "title": "Protocol",
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "ethicsApproval": {
      "title": "Ethicsapproval",
      "type": "array",
      "items": {
        "$ref": "#/definitions/EthicsApproval"
      }
    },
    "acknowledgement": {
      "title": "Acknowledgement",
      "type": "string"
    },
    "access": {
      "title": "Access",
      "type": "array",
      "items": {
        "$ref": "#/definitions/AccessRequirements"
      }
    },
    "relatedResource": {
      "title": "Relatedresource",
      "type": "array",
      "items": {
        "$ref": "#/definitions/Resource"
      }
    },
    "url": {
      "title": "Url",
      "description": "permalink to the dandiset",
      "readonly": true,
      "type": "string",
      "minLength": 1,
      "maxLength": 65536,
      "format": "uri"
    },
    "repository": {
      "title": "Repository",
      "description": "location of the ",
      "readonly": true,
      "type": "string",
      "minLength": 1,
      "maxLength": 65536,
      "format": "uri"
    },
    "assetsSummary": {
      "title": "Assetssummary",
      "readonly": true,
      "allOf": [
        {
          "$ref": "#/definitions/AssetsSummary"
        }
      ]
    },
    "manifestLocation": {
      "title": "Manifestlocation",
      "readonly": true,
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1,
        "maxLength": 65536,
        "format": "uri"
      }
    },
    "version": {
      "title": "Version",
      "readonly": true,
      "type": "string"
    },
    "datePublished": {
      "title": "Datepublished",
      "readonly": true,
      "type": "string",
      "format": "date"
    },
    "publishedBy": {
      "title": "Publishedby",
      "readonly": true,
      "type": "string",
      "minLength": 1,
      "maxLength": 65536,
      "format": "uri"
    },
    "doi": {
      "title": "Doi",
      "readonly": true,
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "string",
          "minLength": 1,
          "maxLength": 65536,
          "format": "uri"
        }
      ]
    }
